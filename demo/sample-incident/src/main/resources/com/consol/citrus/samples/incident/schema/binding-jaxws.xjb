<jaxws:bindings xmlns="https://jakarta.ee/xml/ns/jaxb"
          xmlns:jaxb="https://jakarta.ee/xml/ns/jaxb"
          xmlns:jaxws="https://jakarta.ee/xml/ns/jaxws"
          xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
          xmlns:xs="http://www.w3.org/2001/XMLSchema"
          jaxb:version="3.0">

  <jaxws:bindings node="wsdl:definitions/wsdl:types/xs:schema[@targetNamespace='http://www.citrusframework.org/schema/samples/IncidentManager/v1']">
    <jaxb:globalBindings>
      <jaxb:javaType
              name="java.util.Calendar"
              xmlType="xs:dateTime"
              parseMethod="jakarta.xml.bind.DatatypeConverter.parseDateTime"
              printMethod="jakarta.xml.bind.DatatypeConverter.printDateTime"/>
    </jaxb:globalBindings>
  </jaxws:bindings>
</jaxws:bindings>
